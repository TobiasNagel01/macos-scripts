function updatemintscripts() {( curl -H 'Cache-Control: no-cache, no-store' -s https://raw.githubusercontent.com/TobiasNagel01/macos-scripts/main/scripts?cacheclear > /dev/null && curl -H 'Cache-Control: no-cache, no-store' -s https://raw.githubusercontent.com/TobiasNagel01/macos-scripts/main/scripts > ~/.zshrc_mint && sleep 3 && source ~/.zshrc && echo "Scripts updated" )}

alias copyssh="cat ~/.ssh/id_rsa.pub | pbcopy"
